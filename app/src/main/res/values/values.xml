<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="jsonViewer_textColorBool">#ff8c00</color>
    <color name="jsonViewer_textColorNull">#808080</color>
    <color name="jsonViewer_textColorNumber">#ee422e</color>
    <color name="jsonViewer_textColorString">#008000</color>
    <color name="jsonViewer_textColorKey">#5E35B1</color>
    <declare-styleable name="JsonViewer">
        <attr format="color" name="textColorString"/>
        <attr format="color" name="textColorBool"/>
        <attr format="color" name="textColorNull"/>
        <attr format="color" name="textColorNumber"/>
        <attr format="color" name="textColorKey"/>
    </declare-styleable>
    <style name="JsonViewer.TextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">monospace</item>
    </style>
</resources>